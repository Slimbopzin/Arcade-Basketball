-- Spny Hub - Fully Mobile-Friendly GUI with Logo Switcher
-- LocalScript in StarterGui

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- RemoteEvent for Auto Green
local AutoGreenEvent = ReplicatedStorage:WaitForChild("AutoGreenEvent")

-- === SCREEN GUI ===
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "SpnyHub"
screenGui.ResetOnSpawn = false
screenGui.Enabled = true
screenGui.Parent = player:WaitForChild("PlayerGui")

-- === MAIN FRAME ===
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 250, 0, 400)
mainFrame.Position = UDim2.new(0.05, 0, 0.1, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
mainFrame.BorderSizePixel = 0
mainFrame.Active = true
mainFrame.Draggable = true
mainFrame.AnchorPoint = Vector2.new(0,0)
mainFrame.Parent = screenGui

local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 30)
title.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
title.Text = "Spny Hub"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamBold
title.TextSize = 16
title.Parent = mainFrame

-- === WALK SPEED SLIDER ===
local sliderBg = Instance.new("Frame")
sliderBg.Size = UDim2.new(0, 200, 0, 10)
sliderBg.Position = UDim2.new(0, 25, 0, 50)
sliderBg.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
sliderBg.BorderSizePixel = 0
sliderBg.Parent = mainFrame

local sliderKnob = Instance.new("Frame")
sliderKnob.Size = UDim2.new(0, 20, 0, 20)
sliderKnob.Position = UDim2.new(0, 0, -0.5, 0)
sliderKnob.BackgroundColor3 = Color3.fromRGB(120, 0, 255)
sliderKnob.BorderSizePixel = 0
sliderKnob.Parent = sliderBg

local speedLabel = Instance.new("TextLabel")
speedLabel.Size = UDim2.new(1, 0, 0, 20)
speedLabel.Position = UDim2.new(0, 0, 0, 80)
speedLabel.BackgroundTransparency = 1
speedLabel.Text = "WalkSpeed: 16"
speedLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
speedLabel.Font = Enum.Font.Gotham
speedLabel.TextSize = 14
speedLabel.Parent = mainFrame

-- === AUTO GREEN TOGGLE ===
local autoGreen = false
local autoGreenBtn = Instance.new("TextButton")
autoGreenBtn.Size = UDim2.new(1, -20, 0, 30)
autoGreenBtn.Position = UDim2.new(0, 10, 0, 120)
autoGreenBtn.BackgroundColor3 = Color3.fromRGB(90, 0, 140)
autoGreenBtn.Text = "Auto Green: OFF"
autoGreenBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
autoGreenBtn.Font = Enum.Font.GothamBold
autoGreenBtn.TextSize = 14
autoGreenBtn.Parent = mainFrame

autoGreenBtn.MouseButton1Click:Connect(function()
	autoGreen = not autoGreen
	autoGreenBtn.Text = "Auto Green: " .. (autoGreen and "ON" or "OFF")
	AutoGreenEvent:FireServer(autoGreen)
end)

-- === SPINBOT ===
local spinbot = false
local spinSpeed = 5
local spinBtn = Instance.new("TextButton")
spinBtn.Size = UDim2.new(1, -20, 0, 30)
spinBtn.Position = UDim2.new(0, 10, 0, 160)
spinBtn.BackgroundColor3 = Color3.fromRGB(0, 120, 140)
spinBtn.Text = "Spinbot: OFF"
spinBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
spinBtn.Font = Enum.Font.GothamBold
spinBtn.TextSize = 14
spinBtn.Parent = mainFrame

spinBtn.MouseButton1Click:Connect(function()
	spinbot = not spinbot
	spinBtn.Text = "Spinbot: " .. (spinbot and "ON" or "OFF")
end)

local spinSliderBg = Instance.new("Frame")
spinSliderBg.Size = UDim2.new(0, 200, 0, 10)
spinSliderBg.Position = UDim2.new(0, 25, 0, 200)
spinSliderBg.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
spinSliderBg.BorderSizePixel = 0
spinSliderBg.Parent = mainFrame

local spinSliderKnob = Instance.new("Frame")
spinSliderKnob.Size = UDim2.new(0, 20, 0, 20)
spinSliderKnob.Position = UDim2.new(0, 0, -0.5, 0)
spinSliderKnob.BackgroundColor3 = Color3.fromRGB(255, 140, 0)
spinSliderKnob.BorderSizePixel = 0
spinSliderKnob.Parent = spinSliderBg

-- === SLIDER FUNCTION ===
local function makeSlider(sliderBg, sliderKnob, min, max, callback)
	local dragging = false
	sliderKnob.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
		end
	end)
	UserInputService.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = false
		end
	end)
	UserInputService.InputChanged:Connect(function(input)
		if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
			local relativeX = math.clamp(input.Position.X - sliderBg.AbsolutePosition.X, 0, sliderBg.AbsoluteSize.X)
			sliderKnob.Position = UDim2.new(0, relativeX - sliderKnob.Size.X.Offset/2, sliderKnob.Position.Y.Scale, sliderKnob.Position.Y.Offset)
			local percent = relativeX / sliderBg.AbsoluteSize.X
			local value = math.floor(min + (max - min) * percent)
			callback(value)
		end
	end)
end

makeSlider(sliderBg, sliderKnob, 16, 100, function(value)
	speedLabel.Text = "WalkSpeed: " .. value
	humanoid.WalkSpeed = value
end)

makeSlider(spinSliderBg, spinSliderKnob, 1, 50, function(value)
	spinSpeed = value
end)

RunService.RenderStepped:Connect(function()
	if spinbot and character:FindFirstChild("HumanoidRootPart") then
		character.HumanoidRootPart.CFrame *= CFrame.Angles(0, math.rad(spinSpeed), 0)
	end
end)

-- === COLLAPSIBLE GUI ===
local toggleBtn = Instance.new("TextButton")
toggleBtn.Size = UDim2.new(0, 50, 0, 50)
toggleBtn.Position = UDim2.new(0, 20, 0, 100)
toggleBtn.BackgroundColor3 = Color3.fromRGB(120, 0, 255)
toggleBtn.Text = "Spny"
toggleBtn.TextColor3 = Color3.fromRGB(255,255,255)
toggleBtn.Font = Enum.Font.GothamBold
toggleBtn.TextSize = 14
toggleBtn.Visible = false
toggleBtn.Parent = screenGui
toggleBtn.ZIndex = 10

local guiOpen = true

local function collapseGUI()
	mainFrame.Visible = false
	toggleBtn.Visible = true
	guiOpen = false
end

local function expandGUI()
	mainFrame.Visible = true
	toggleBtn.Visible = false
	guiOpen = true
end

toggleBtn.MouseButton1Click:Connect(expandGUI)

local collapseBtn = Instance.new("TextButton")
collapseBtn.Size = UDim2.new(0, 30, 0, 30)
collapseBtn.Position = UDim2.new(1, -35, 0, 5)
collapseBtn.BackgroundColor3 = Color3.fromRGB(180, 0, 180)
collapseBtn.Text = "-"
collapseBtn.TextColor3 = Color3.fromRGB(255,255,255)
collapseBtn.Font = Enum.Font.GothamBold
collapseBtn.TextSize = 18
collapseBtn.Parent = mainFrame
collapseBtn.MouseButton1Click:Connect(collapseGUI)

-- === LOGO SWITCHER ===
local logos = {
	TikTok = "rbxassetid://INSERT_TIKTOK_IMAGE_ID",
	YouTube = "rbxassetid://INSERT_YOUTUBE_IMAGE_ID",
	Custom = "rbxassetid://INSERT_CUSTOM_IMAGE_ID"
}

local logoFrame = Instance.new("Frame")
logoFrame.Size = UDim2.new(1, -20, 0, 100)
logoFrame.Position = UDim2.new(0, 10, 0, 250)
logoFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
logoFrame.BorderSizePixel = 0
logoFrame.Parent = mainFrame

local logoTitle = Instance.new("TextLabel")
logoTitle.Size = UDim2.new(1, 0, 0, 20)
logoTitle.Position = UDim2.new(0, 0, 0, 0)
logoTitle.BackgroundTransparency = 1
logoTitle.Text = "Select Logo"
logoTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
logoTitle.Font = Enum.Font.GothamBold
logoTitle.TextSize = 14
logoTitle.Parent = logoFrame

local buttonY = 25
for name, assetId in pairs(logos) do
	local btn = Instance.new("TextButton")
	btn.Size = UDim2.new(1, -20, 0, 20)
	btn.Position = UDim2.new(0, 10, 0, buttonY)
	btn.BackgroundColor3 = Color3.fromRGB(120, 0, 255)
	btn.TextColor3 = Color3.fromRGB(255,255,255)
	btn.Font = Enum.Font.Gotham
	btn.TextSize = 14
	btn.Text = name
	btn.Parent = logoFrame

	btn.MouseButton1Click:Connect(function()
		if player:FindFirstChild("Logo") then
			player.Logo:Destroy()
		end
		local logo = Instance.new("ImageLabel")
		logo.Name = "Logo"
		logo.Size = UDim2.new(0, 30, 0, 30)
		logo.Position = UDim2.new(1, 5, 0, -5)
		logo.BackgroundTransparency = 1
		logo.Image = assetId
		logo.Parent = screenGui
	end)

	buttonY = buttonY + 25
end